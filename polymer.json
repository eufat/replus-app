{
    "entrypoint": "index.html",
    "shell": "src/components/main-app.js",
    "sources": ["images/**/*"],
    "extraDependencies": [
        "manifest.json",
        "node_modules/@webcomponents/webcomponentsjs/**",
        "push-manifest.json"
    ],
    "builds": [
        {
            "preset": "es6-unbundled",
            "addServiceWorker": true,
            "addPushManifest": true
        }
    ],
    "moduleResolution": "node",
    "npm": true
}
