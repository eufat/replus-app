{
    "entrypoint": "index.html",
    "shell": "src/main-app.js",
    "fragments": [
        "src/remote-devices.js",
        "src/remote-rooms.js",
        "src/remote-settings.js",
        "src/vision-events.js",
        "src/vision-streams.js",
        "src/vision-settings.js"
    ],
    "sources": ["src/main-app.js", "manifest/**", "package.json"],
    "extraDependencies": [
        "manifest.json",
        "node_modules/@webcomponents/webcomponentsjs/**",
        "node_modules/redux/dist/**"
    ],
    "builds": [
        {
            "name": "esm-bundled",
            "browserCapabilities": ["es2015", "modules"],
            "js": {
                "minify": true
            },
            "css": {
                "minify": true
            },
            "html": {
                "minify": true
            },
            "bundle": true,
            "addServiceWorker": true
        },
        {
            "name": "es6-bundled",
            "browserCapabilities": ["es2015"],
            "js": {
                "minify": true,
                "transformModulesToAmd": true
            },
            "css": {
                "minify": true
            },
            "html": {
                "minify": true
            },
            "bundle": true,
            "addServiceWorker": true
        },
        {
            "name": "es5-bundled",
            "js": {
                "compile": true,
                "minify": true,
                "transformModulesToAmd": true
            },
            "css": {
                "minify": true
            },
            "html": {
                "minify": true
            },
            "bundle": true,
            "addServiceWorker": true
        }
    ],
    "moduleResolution": "node",
    "npm": true
}
